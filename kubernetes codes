# pod.yml file begins here

apiVersion: v1
kind: Pod
metadata:
  name: ecomm
  labels:
    appType: web
    appTech: html
spec:
  containers:
    - name: web
      image: nginx
      ports:
        - name: web
          containerPort: 80
          protocol: TCP
          
# pod.yml ends here


